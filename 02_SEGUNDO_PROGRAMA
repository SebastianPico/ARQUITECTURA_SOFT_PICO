from typing import List

class Orden:
    def __init__(self, id_orden: str, nombre_servicio: str, cantidad: float, valor_unitario: float):
        self.id_orden = id_orden
        self.nombre_servicio = nombre_servicio
        self.cantidad = cantidad
        self.valor_unitario = valor_unitario
    
def calcular_total(ordenes: List[Orden]) -> float:
    return sum(ord.cantidad * ord.valor_unitario for ord in ordenes)
    
def main():
    ordenes = []
    try:
        cantidad_ordenes = int(input("Digite la cantidad de órdenes: "))
    except ValueError:
        print("Error. Valor no válido para cantidad de órdenes.")
        return
    
    for i in range(cantidad_ordenes):
        id_orden = input("Digite el ID de la orden: ")
        nombre_servicio = input("Digite el nombre del servicio: ")
        try:
            cantidad = float(input("Digite la cantidad: "))
            valor = float(input("Digite el valor unitario: "))
        except ValueError:
            print("Error. Valor no válido para cantidad o valor unitario.")
            return
        orden = Orden(id_orden, nombre_servicio, cantidad, valor)
        ordenes.append(orden)
    
    total_factura = calcular_total(ordenes)
    print(f"El valor total de las órdenes es: {total_factura}")
    
if __name__ == "__main__":
    main()
